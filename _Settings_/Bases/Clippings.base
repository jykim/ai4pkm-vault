views:
  - type: table
    name: Table
    filters:
      and:
        - file.path.startsWith("Ingest/Clippings")
    order:
      - file.ctime
      - file.name
      - author
      - source
    sort:
      - property: file.ctime
        direction: DESC
      - property: title
        direction: ASC
    columnSize:
      file.ctime: 94
      note.author: 185
